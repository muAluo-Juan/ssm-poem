<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 输出日志配置 -->
		<setting name="logImpl" value="LOG4J" />  
	</settings>
	<!-- 别名 -->
	<typeAliases>
		<typeAlias type="model.MyUser" alias="MyUser" />
		<typeAlias type="po.MyUserTable" alias="MyUserTable" />
		<typeAlias type="po.NormalUser" alias="NormalUser"/>
		<typeAlias type="po.Administrator" alias="Admin"/>
	</typeAliases>

    <!-- 告诉 MyBatis到哪里去找映射文件 -->
	 <mappers>
        <mapper resource="mybatis/MyUserTableMapper.xml"/>
 	</mappers>
</configuration>
